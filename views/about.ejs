<!DOCTYPE html>
<html lang="en">
  <head>
      <% include partials/header.ejs %>
      <% include partials/scripts.ejs %>
      <% include partials/background.ejs %>
    <div>
      <% include partials/about.ejs %>
    </div>
  <% include partials/footer.ejs %>
</body>
  <script type="text/javascript" src="javascript/ImageSlider.js"></script>
  <script>
    function checkCopyRightOverlap(){
      var copyright = document.getElementById('footer-copyright');
      var text = document.getElementById('about-me-text');
      if(intersect(copyright), intersect(text)){
        copyright.style.opacity = 0;
      }else{
        copyright.style.opacity = 1;
      }
    }
    $(document).ready(function(){
      checkCopyRightOverlap();
    });
    window.onscroll = checkCopyRightOverlap();
  </script>
</html>
